{
  "name": "@enfyra/sdk-nuxt",
  "version": "0.3.3",
  "description": "Nuxt SDK for Enfyra CMS",
  "repository": {
    "type": "git",
    "url": "https://github.com/dothinh115/enfyra-sdk-nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/dothinh115/enfyra-sdk-nuxt/issues"
  },
  "homepage": "https://github.com/dothinh115/enfyra-sdk-nuxt#readme",
  "main": "./dist/module.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "keywords": [
    "nuxt",
    "enfyra",
    "cms",
    "sdk"
  ],
  "author": "Enfyra Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxi dev playground",
    "build": "nuxt-module-build build && npx tsc -p tsconfig.build.json && node scripts/copy-composables-types.js",
    "prepack": "nuxt-module-build build && npx tsc -p tsconfig.build.json && node scripts/copy-composables-types.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.18.1",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "cookie": "^0.6.0",
    "glob": "^8.1.0",
    "h3": "^1.15.4",
    "jwt-decode": "^4.0.0",
    "nuxt": "^3.18.1",
    "ofetch": "^1.3.3",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.4",
    "@types/cookie": "^0.6.0",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4"
  }
}
