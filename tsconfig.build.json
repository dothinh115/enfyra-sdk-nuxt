{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "moduleResolution": "node"
  },
  "include": [
    "src/types/**/*"
  ],
  "exclude": [
    "src/runtime/**/*",
    "src/utils/server/**/*",
    "node_modules",
    "dist"
  ]
}